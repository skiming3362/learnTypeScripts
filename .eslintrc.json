{
    "extends": [
        "eslint-config-alloy/typescript"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": false
        }
    },
    "globals": {

    },
    "rules": {
        "no-unused-vars": "off",
        "no-trailing-spaces": 0,
        "no-useless-constructor": 0,
        "no-empty-function": 0
    }
}